<h1>Books</h1>
<% @books.each do |book| %>

<table>
<tr>
  <th> <%= book.title %> </th>
  <th> <%= book.body %> </th>
  <span>
   <th> <%= link_to'show',@rout_path %> </th>
  </span>
</tr>
</table>

<% end %>

<h2>New book</h2>
<%= form_with model:@book, local:true do |f| %>
<label>Title</label>
<%= f.text_field :title %>

<label>Body</label>
<%= f.text_area :body %>

<%= f.submit 'Create Book' %>

<% end %>

